{
  "cli": {
    "version": ">= 16.31.0",
    "appVersionSource": "remote"
  },
  "build": {
    "customer-development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "app.json"
      }
    },
    "pharmacist-development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "app.pharmacist.json"
      }
    },
    "customer-preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "app.json"
      }
    },
    "pharmacist-preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "app.pharmacist.json"
      }
    },
    "customer-production": {
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "app.json"
      }
    },
    "pharmacist-production": {
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "app.pharmacist.json"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
